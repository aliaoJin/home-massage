{
    "pages": [
        {
            "path": "pages/tabBar/home",
            "style": {
                "navigationBarTitleText": " 首页",
                "enablePullDownRefresh": false,
                "navigationBarBackgroundColor": "#409eff",
                "navigationBarTextStyle": "white",
                "navigationStyle": "custom"
            }
        },
        {
            "path": "pages/tabBar/index",
            "style": {
                "navigationBarTitleText": "订单",
                "enablePullDownRefresh": false,
                "navigationBarBackgroundColor": "#ffffff"
            }
        },
        {
            "path": "pages/tabBar/my",
            "style": {
                "navigationBarTitleText": "我的", 
                "navigationBarBackgroundColor": "#ffffff"
            }
        }
    ],
    "subPackages": [
        {
            "root": "subPack/",
            "pages": [
                {
                    "path": "home/index",
                    "style": {
                        "navigationBarTitleText": "",
                        "navigationBarBackgroundColor": "#ffffff"
                    }
                },
                 {
                    "path": "webView/index",
                    "style": {
                        "navigationBarTitleText": " ",
                        "navigationBarBackgroundColor": "#ffffff"
                    }
                }
            ]
        }
    ],
    "tabBar": {
        "color": "#8a8a8a",
        "selectedColor": "#000000",
        "borderStyle": "black",
        "backgroundColor": "#ffffff",
        "iconfontSrc": "./static/fonts/iconfont.ttf",
        "list": [
            {
                "pagePath": "pages/tabBar/home",
                "text": "首页",
                "iconPath": "/static/tabBarIcon/a2.png",
                "selectedIconPath": "/static/tabBarIcon/a.png"
            },
            {
                "pagePath": "pages/tabBar/index",
                "text": "订单",
                "iconPath": "/static/tabBarIcon/b2.png",
                "selectedIconPath": "/static/tabBarIcon/b.png"
            },
            {
                "pagePath": "pages/tabBar/my",
                "text": "我的",
                "iconPath": "/static/tabBarIcon/c2.png",
                "selectedIconPath": "/static/tabBarIcon/c.png"
            }
        ]
    },
    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "uni-app",
        "navigationBarBackgroundColor": "#F8F8F8",
        "backgroundColor": "#F8F8F8",
        "homeButton": true
    },
    "preloadRule": {
        "pages/tabBar/index": {
            "network": "all",
            "packages": ["subPack/"]
        }
    },
    "easycom": {
        "autoscan": true,
        "custom": {
            "^nut-(.*)?-(.*)": "nutui-uniapp/components/$1$2/$1$2.vue",
            "^nut-(.*)": "nutui-uniapp/components/$1/$1.vue",
            "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
        }
    },
     "permission": {
        "subscribeMessage": {
            "desc": "你的订阅消息使用目的说明"
        }
    }
}
